  "type": "object",
  "additionalProperties": false,
  "properties": {
    "inGameName": {
      "type": "string"
    },
    "gameID": {
      "type": "object"
    },
    "email": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    }
  },
